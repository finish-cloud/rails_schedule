<h2>ユーザー情報一覧</h2>

<% if flash.notice %>
<p class="notice"><%= flash.notice %></p>
<% end %>

<table>
  <thead>
    <tr>
      <th>id</th>
      <th>名前</th>
      <th>メールアドレス</th>
      <th>年齢</th>
      <th>一言紹介</th>
      <th>更新日</th>
      <th>作成日</th>
    </tr>
  </thead>
  <tbody>
    <% @posts.each do |post| %>
    <tr>
      <td><%= post.id %></td>
      <td><%= post.title %></td>
      <td><%=  %></td>
      <td><%=  %></td>
      <td><%=  %></td>
      <td><%= post.updated_at %></td>
      <td><%= post.created_at %></td>
    </tr>
    <% end %>
  </tbody>
</table>

<p><%= link_to "スケジュール新規登録", :new_post %></p>